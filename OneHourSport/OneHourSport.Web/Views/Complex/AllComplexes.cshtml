@model PagedList.IPagedList<OneHourSport.Web.Models.Complex.ComplexDisplayViewModel>
@{
    Layout = "~/Views/Shared/_Layout.cshtml";
    var index = 1;
}

@foreach (var complex in Model)
{
    complex.Index = index;
    Html.RenderPartial("~/Views/Complex/_ComplexDisplayPartial.cshtml", complex);
    index++;
}

@Html.Partial("_PagingPartial", Model)