@model PagedList.IPagedList<OneHourSport.Web.Models.Field.FieldDisplayViewModel>
@{
    Layout = "~/Views/Shared/_Layout.cshtml";
    var index = 1;
}

@foreach (var item in Model)
{
    item.Index = index;
    Html.RenderPartial("~/Views/Field/_FieldDisplayPartial.cshtml", item);
    index++;
}

@Html.Partial("_PagingPartial", Model)